{
  "compilerOptions": {
    "baseUrl": "./src",
    "outDir": "./dist",
    "target": "ES2019",
    "module": "esnext",
    "moduleResolution": "node",
    "jsx": "react",
    "noImplicitAny": true,
    "allowSyntheticDefaultImports": true,
    "paths": {
      "assets/*": [
        "src/assets/*"
      ]
    },
    "sourceMap": true
  },
  "exclude": [
    "node_modules",
    "dist",
  ]
}